    "path-to-regexp": "^6.2.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "sass": "^1.35.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "vite": "2.4.2",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-style-import": "^1.0.0",
    "vue-eslint-parser": "^7.6.0"
